namespace Serialization;

struct Point {
  row: int;
  column: int;
}

struct Range {
  start: Point;
  end: Point;
}

table Marker {
  id: int;
  range: Range;
  reversed: bool;
  tailed: bool;
  valid: bool;
  invalidate: string;
  properties: string;
}

table MarkerLayer {
  id: int;
  markers: [Marker];
}

table MarkersSnapshot {
  markerLayers: [MarkerLayer];
}

root_type MarkersSnapshot;
